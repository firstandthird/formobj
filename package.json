{
  "name": "formobj",
  "version": "3.0.0",
  "description": "Turns form values into json and back into form values",
  "main": "dist/formobj.js",
  "keywords": [
    "browser",
    "form",
    "json"
  ],
  "scripts": {
    "build": "scriptkit build",
    "dev": "scriptkit dev",
    "test": "cat test/formobj.test.dist.js | tape-run --browser phantomjs --render tap-spec",
    "prepublish": "scriptkit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/firstandthird/formobj.git"
  },
  "author": "First+Third",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/firstandthird/formobj/issues"
  },
  "homepage": "https://github.com/firstandthird/formobj#readme",
  "dependencies": {
    "domassist": "^1.5.0"
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "extends": "firstandthird"
  },
  "module": "formobj.js",
  "scriptkit": {
    "scripts": {
      "babel": {
        "exclude": [
          "node_modules/tape-rollup/*"
        ]
      },
      "files": {
        "dist/formobj.js": "formobj.js",
        "test/formobj.test.dist.js": "test/formobj.test.js"
      }
    }
  },
  "devDependencies": {
    "eslint-config-firstandthird": "^3.2.0",
    "eslint-plugin-import": "^2.2.0",
    "phantomjs": "^2.1.7",
    "scriptkit": "0.0.22",
    "static-server": "^2.0.4",
    "tap-spec": "^4.1.1",
    "tape-rollup": "4.6.4",
    "tape-run": "^2.2.0"
  }
}
